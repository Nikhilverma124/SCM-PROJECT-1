#include<stdio.h>
#include<conio.h>
int Menu();
void Bus();
void Cycle();
void Riksha();
void Delete();
void ShowDetail();
int nor=0,nob=0,noc=0,amount=0,count=0;
void main()
{
 while(1)
 {
  switch(Menu())
  {
   case 1:
  Bus();
  break;
   case 2:
  Cycle();
  break;
   case 3:
  Riksha();
  break;
   case 4:
  ShowDetail();
  break;
   case 5:
  Delete();
  break;
   case 6:
  exit(0);
   default :
 printf("\nInvalid choice:");

  }
  
 }

}


